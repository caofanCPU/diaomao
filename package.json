    "next.config.ts",
    "postcss.config.mjs",
    "tailwind.config.ts",
    "source.config.ts",
    "tsconfig.json",
    "tsconfig.node.json",
    "!**/scripts/**",
    "!**/*.test.{js,ts}",
    "!**/*.log"
  ]
