{
  "name": "@windrun-huaiin/diaomao",
  "version": "12.0.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "keywords": [
    "fumadocs",
    "clerk",
    "nextjs",
    "template",
    "diaomao",
    "react"
  ],
  "scripts": {
    "dev": "prisma generate && next dev",
    "build": "pnpm generate-blog-index && prisma generate && next build",
    "build:dev": "pnpm generate-blog-index && prisma generate && next build",
    "build:prod": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "deep-clean": "dev-scripts deep-clean",
    "d8": "pnpm run deep-clean",
    "easy-changeset": "dev-scripts easy-changeset",
    "dj": "pnpm run easy-changeset && pnpm changeset status",
    "djv": "pnpm changeset version",
    "djvp": "pnpm changeset publish",
    "generate-blog-index": "dev-scripts generate-blog-index -v",
    "check-translations": "dev-scripts check-translations -v",
    "clean-translations": "dev-scripts clean-translations -v",
    "remove-translations": "dev-scripts clean-translations --remove -v",
    "core-list-route": "dev-scripts backend-core routes:list",
    "core-sync-route": "dev-scripts backend-core routes:sync",
    "core-sync-schema": "dev-scripts backend-core prisma:sync",
    "core-sync-sql": "dev-scripts backend-core migrations:sync --schema diaomao  --dest database",
    "whoareyou": "dev-scripts generate-nextjs-architecture -v",
    "windrun": "pnpm remove @windrun-huaiin/base-ui @windrun-huaiin/lib @windrun-huaiin/third-ui @windrun-huaiin/backend-core @windrun-huaiin/dev-scripts  && pnpm add @windrun-huaiin/base-ui@latest @windrun-huaiin/lib@latest @windrun-huaiin/third-ui@latest @windrun-huaiin/backend-core@latest && pnpm add -D @windrun-huaiin/dev-scripts@latest",
    "windrun3": "pnpm remove @windrun-huaiin/third-ui && pnpm add @windrun-huaiin/third-ui@latest",
    "windrunC": "pnpm remove @windrun-huaiin/backend-core && pnpm add @windrun-huaiin/backend-core@latest"
  },
  "dependencies": {
    "@clerk/localizations": "^3.28.0",
    "@clerk/nextjs": "^6.35.1",
    "@clerk/themes": "^2.4.36",
    "@clerk/types": "^4.100.0",
    "@fingerprintjs/fingerprintjs": "^4.6.2",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/typography": "^0.5.19",
    "@types/mdx": "^2.0.13",
    "@windrun-huaiin/backend-core": "^12.0.0",
    "@windrun-huaiin/base-ui": "^12.0.0",
    "@windrun-huaiin/lib": "^12.0.0",
    "@windrun-huaiin/third-ui": "^12.0.0",
    "autoprefixer": "^10.4.22",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "fumadocs-core": "16.0.9",
    "fumadocs-docgen": "3.0.4",
    "fumadocs-mdx": "13.0.6",
    "fumadocs-typescript": "4.0.13",
    "fumadocs-ui": "16.0.9",
    "katex": "^0.16.25",
    "lucide-react": "^0.525.0",
    "mermaid": "^11.12.1",
    "next": "16.0.10",
    "next-intl": "^4.4.0",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-medium-image-zoom": "^5.4.0",
    "rehype-katex": "^7.0.1",
    "remark": "^15.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-mdx": "^3.1.1",
    "shiki": "^3.15.0",
    "stripe": "20.0.0",
    "svix": "^1.81.0",
    "swiper": "^12.0.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@prisma/client": "^6.19.0",
    "@tailwindcss/cli": "^4.1.17",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/hast": "^3.0.4",
    "@types/node": "^22.17.2",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@types/react-medium-image-zoom": "^3.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "^8.46.4",
    "@windrun-huaiin/dev-scripts": "^12.0.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "fast-glob": "^3.3.3",
    "nprogress": "^0.2.0",
    "postcss": "^8.5.6",
    "prisma": "^6.19.0",
    "tailwindcss": "^4.1.17",
    "ts-morph": "^27.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "unist-util-visit": "^5.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@clerk/shared",
      "@parcel/watcher",
      "@prisma/client",
      "@prisma/engines",
      "@swc/core",
      "@tailwindcss/oxide",
      "core-js",
      "esbuild",
      "prisma",
      "sharp",
      "unrs-resolver"
    ],
    "patchedDependencies": {
      "fumadocs-ui@16.0.9": "patches/fumadocs-ui@16.0.9.patch",
      "fumadocs-core@16.0.9": "patches/fumadocs-core@16.0.9.patch"
    }
  },
  "files": [
    ".changeset",
    "!.changeset/d8-template.mdx",
    "patches",
    "src",
    "public",
    ".gitignore",
    "messages",
    ".env.local.txt",
    "eslint.config.js",
    "components.json",
    "dev-scripts.config.json",
    "LICENSE",
    "next-env.d.ts",
    "next.config.ts",
    "postcss.config.mjs",
    "tailwind.config.ts",
    "source.config.ts",
    "tsconfig.json",
    "tsconfig.node.json",
    "!**/scripts/**",
    "!**/*.test.{js,ts}",
    "!**/*.log"
  ]
}
