{
  "i18n": {
    "locales": ["en", "zh"],
    "defaultLocale": "en",
    "messageRoot": "messages"
  },
  "scan": {
    "include": ["src/**/*.{tsx,ts,jsx,js}"],
    "exclude": ["src/**/*.test.ts", "src/**/*.test.tsx", "src/**/*.d.ts", "node_modules/**"]
  },
  "blog": {
    "mdxDir": "src/mdx/blog",
    "outputFile": "index.mdx",
    "metaFile": "meta.json",
    "iocSlug": "ioc",
    "prefix": "blog"
  },
  "output": {
    "logDir": "logs",
    "verbose": false
  },
  "architectureConfig": {
    ".": "Diaomao Project",
    "fumadocs-ui@15.3.3.patch": "fumadocs-ui版本15.3.3补丁",
    "fumadocs-ui@16.0.9.patch": "fumadocs-ui版本16.0.9补丁",
    "fumadocs-core@16.0.9.patch": "fumadocs-core版本16.0.9补丁",
    ".env.local.txt": "config example"
  }
}
